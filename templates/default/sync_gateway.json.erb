{
  "interface":":4984",
  "adminInterface":":4985",
  "log":["REST"],
  "databases":{
    "frontlog":{
      "server":"<%= node['couchbase']['sync-gateway']['server_url'] %>",
      "bucket":"frontlog",
      "sync":`function (doc, oldDoc) {
        channel(doc.channels);
        if (oldDoc && !oldDoc._deleted) {
          requireAccess(oldDoc.channels);
        }
        if (doc && !doc._deleted) {
          requireAccess(doc.channels);
        }
      }`
    }
  }
}